{
  "name": "fix.buzz.node",
  "version": "0.0.1",
  "description": "fix.buzz backend",
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "start": "babel-node server.js",
    "dev": "./node_modules/.bin/nodemon server.js --exec babel-node",
    "build": "webpack",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register ./test/*.js",
    "migrate": "./node_modules/.bin/pg-migrate $@ --database-url-var DATABASE_URL",
    "migrate-test": "./node_modules/.bin/pg-migrate $@ --database-url-var DATABASE_URL_TEST"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:echowaves/fix.buzz.node.git"
  },
  "author": "Echowaves <dmitry@echowaves.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/echowaves/fix.buzz.node/issues"
  },
  "homepage": "https://github.com/echowaves/fix.buzz.node/wiki",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "bcrypt-as-promised": "^1.1.0",
    "dotenv": "^4.0.0",
    "jwt-simple": "^0.5.1",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.1.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.1.0",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "node-pg-migrate": "^1.2.1",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.4",
    "pg-native": "^1.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.3",
