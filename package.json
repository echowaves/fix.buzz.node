    "babel-preset-stage-3": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^3.2.1",
    "jsdom": "^9.12.0",
    "koa-webpack": "^0.3.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "standard": "^9.0.1",
    "supertest": "^3.0.0",
    "webpack": "^2.3.3"
  }
